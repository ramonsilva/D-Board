<% tasks.each do |task| %>
  <div class='task' id="<%= task.id %>">
    <%= task.description %>
    <br />
    <%= link_to 'Edit', edit_story_task_path(story, task) %>
    |
    <%= link_to 'Destroy', story_task_path(story, task), method: :delete, data: { confirm: 'Are you sure?' } %>
  </div>
<% end %>
